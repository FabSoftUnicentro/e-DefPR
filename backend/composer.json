{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "Waavi/Sanitizer": "^1.0",
        "barryvdh/laravel-cors": "^0.11.0",
        "benjamincrozat/laravel-dropbox-driver": "^2.0",
        "fideloper/proxy": "^4.0",
        "freshbitsweb/laravel-log-enhancer": "^1.0",
        "fzaninotto/faker": "^1.4",
        "guzzlehttp/guzzle": "^6.3",
        "jeroennoten/laravel-adminlte": "^1.24",
        "kris/laravel-form-builder": "^1.15",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "laravellegends/pt-br-validator": "5.1.*",
        "sentry/sentry-laravel": "^0.11.0",
        "spatie/laravel-backup": "^5.11",
        "spatie/laravel-permission": "^2.12"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
